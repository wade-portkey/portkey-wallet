<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.aelf.portkey" >

    <uses-sdk android:minSdkVersion="26" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_MEDIA_IMAGES" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BaseTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="31" >
        <activity
            android:name="io.aelf.portkey.components.activities.DefaultReactActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait"
            android:theme="@style/ReactActivityAnimation" />
        <activity android:name="com.facebook.react.devsupport.DevSettingsActivity" />

        <service android:name="io.aelf.portkey.components.services.GeneralJSMethodService" />

        <provider
            android:name="io.aelf.core.InitProvider"
            android:authorities="dollar_openBracket_applicationId_closeBracket.init.provider"
            android:exported="false"
            android:initOrder="1" />
    </application>

</manifest>